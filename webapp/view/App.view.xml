<mvc:View
   controllerName="ui5.walkthrough.controller.App"
   xmlns="sap.m"
   xmlns:f="sap.f"
   xmlns:mvc="sap.ui.core.mvc"
   displayBlock="true">
   <Shell>
     <App >
      <pages>
        <f:DynamicPage >
            <f:title>
               <f:DynamicPageTitle >
                  <f:heading>
                      <Title text="{i18n>homePageTitle}"/>
                  </f:heading>
               </f:DynamicPageTitle>
            </f:title>
              
            <f:content>
            <mvc:XMLView viewName="ui5.walkthrough.view.HelloPanel"/>
          
            </f:content>
            
        </f:DynamicPage>
      </pages>
  </App>
  </Shell>
</mvc:View>